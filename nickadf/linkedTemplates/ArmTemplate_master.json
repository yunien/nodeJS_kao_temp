{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "nickadf"
		},
		"AzureFileStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureFileStorage1'"
		},
		"AzureSynapseAnalytics2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSynapseAnalytics2'"
		},
		"LakeLinkedService_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LakeLinkedService'"
		},
		"linkedServiceForCSV_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linkedServiceForCSV'"
		},
		"OriiginCSVLinked_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'OriiginCSVLinked'"
		},
		"LakeLinkedService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://firstsan.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "nickadf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureFileStorage1_connectionString": {
						"value": "[parameters('AzureFileStorage1_connectionString')]"
					},
					"AzureSynapseAnalytics2_connectionString": {
						"value": "[parameters('AzureSynapseAnalytics2_connectionString')]"
					},
					"LakeLinkedService_accountKey": {
						"value": "[parameters('LakeLinkedService_accountKey')]"
					},
					"linkedServiceForCSV_connectionString": {
						"value": "[parameters('linkedServiceForCSV_connectionString')]"
					},
					"OriiginCSVLinked_connectionString": {
						"value": "[parameters('OriiginCSVLinked_connectionString')]"
					},
					"LakeLinkedService_properties_typeProperties_url": {
						"value": "[parameters('LakeLinkedService_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}